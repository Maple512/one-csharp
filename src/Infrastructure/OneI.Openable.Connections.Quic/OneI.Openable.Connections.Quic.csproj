<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>Net8</TargetFramework>
        <EnablePreviewFeatures>true</EnablePreviewFeatures>
        <NoWarn>$(NoWarn);CA1416;</NoWarn>
    </PropertyGroup>

    <Import Project="../../common.props" />

    <ItemGroup>
        <ProjectReference Include="..\OneI.Logable.Abstractions\OneI.Logable.Abstractions.csproj" />
        <ProjectReference Include="..\OneI.Logable.CodeGenerator\OneI.Logable.CodeGenerator.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="false" />
        <ProjectReference Include="..\OneI.Openable.Connection.Abstractions\OneI.Openable.Connection.Abstractions.csproj" />
        <ProjectReference Include="..\OneI\OneI.csproj" />
    </ItemGroup>

    <ItemGroup>
        <Using Include="OneI.Logable.CodeGenerated" />
    </ItemGroup>

    <ItemGroup>
        <Compile Include="..\OneI.Openable.Connections.Shared\**\*.cs" Link="Shared\%(FileName).cs" />
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="..\OneI.Openable.Connections.Shared\obj\Debug\net7.0\.NETCoreApp,Version=v7.0.AssemblyAttributes.cs" />
      <Compile Remove="..\OneI.Openable.Connections.Shared\obj\Debug\net7.0\OneI.Openable.Connections.Shared.AssemblyInfo.cs" />
      <Compile Remove="..\OneI.Openable.Connections.Shared\obj\Debug\net7.0\OneI.Openable.Connections.Shared.GlobalUsings.g.cs" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.Options" Version="7.0.0" />
    </ItemGroup>

</Project>
