<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>$(NetFramework)</TargetFramework>
        <RootNamespace>OneI</RootNamespace>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="DotNext" Version="4.8.1" />
        <PackageReference Include="DotNext.IO" Version="4.8.2" />
        <PackageReference Include="DotNext.Reflection" Version="4.8.1" />
        <PackageReference Include="DotNext.Threading" Version="4.8.2" />
        <PackageReference Include="DotNext.Unsafe" Version="4.8.1" />
        <PackageReference Include="Fody" Version="6.6.4">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Include="InlineIL.Fody" Version="1.7.4" PrivateAssets="All" />
        <PackageReference Include="Perfolizer" Version="0.2.1" />
        <PackageReference Include="ZString" Version="2.5.0" />
    </ItemGroup>

    <Import Project="../../common.props" />

</Project>
